{
  "default_theme": "light",
  "themes": [
    {
      "id": "light",
      "name": "Light",
      "ttk_theme": "clam",
      "palette": {
        "window_background": "#f0f0f0",
        "text_primary": "#111111",
        "text_muted": "#555555",
        "piano_roll": {
          "background": "#f7f7fa",
          "natural_row_fill": "#f7f7fa",
          "accidental_row_fill": "#ffffff",
          "grid_line": "#e0e0e4",
          "measure_line": "#888888",
          "measure_number_text": "#555555",
          "note_fill_sharp": "#9cccfb",
          "note_fill_natural": "#b7dcff",
          "note_outline": "#1e4f87",
          "note_label_text": "#111111",
          "placeholder_text": "#333333",
          "header_text": "#333333",
          "highlight_fill": "#d8ebff",
          "loop_start_line": "#2e7d32",
          "loop_end_line": "#c62828",
          "cursor_primary": "#1e4f87"
        },
        "staff": {
          "background": "#ffffff",
