          "header_text": "#333333",
          "cursor_primary": "#1e4f87",
          "cursor_secondary": "#7a8aa5"
        },
        "listbox": {
          "background": "#ffffff",
          "foreground": "#111111",
          "select_background": "#cde4ff",
          "select_foreground": "#000000"
        },
        "table": {
          "background": "#ffffff",
          "foreground": "#111111",
          "heading_background": "#e6e6e6",
          "heading_foreground": "#111111",
          "row_stripe": "#f7f7f7",
          "selection_background": "#cde4ff",
          "selection_foreground": "#000000"
        }
      },
      "styles": {
        "TFrame": {"background": "#f0f0f0"},
        "TLabelframe": {"background": "#f0f0f0", "foreground": "#111111"},
        "TLabelframe.Label": {"background": "#f0f0f0", "foreground": "#111111"},
        "TLabel": {"background": "#f0f0f0", "foreground": "#111111"},
        "TButton": {"background": "#f0f0f0", "foreground": "#111111"},
        "TCheckbutton": {"background": "#f0f0f0", "foreground": "#111111"},
        "TRadiobutton": {"background": "#f0f0f0", "foreground": "#111111"},
        "TNotebook": {"background": "#f0f0f0"},
        "TNotebook.Tab": {"background": "#e6e6e6", "foreground": "#111111"},
        "TCombobox": {"background": "#f0f0f0", "fieldbackground": "#ffffff", "foreground": "#111111"},
        "TEntry": {"background": "#f0f0f0", "fieldbackground": "#ffffff", "foreground": "#111111"},
        "TScrollbar": {"background": "#f0f0f0"}
      },
      "style_maps": {
        "TButton": {
          "foreground": [["disabled", "#8a8a8a"], ["!disabled", "#111111"]],
          "background": [["disabled", "#d9d9d9"], ["active", "#e6e6e6"], ["pressed", "#d0d0d0"], ["!disabled", "#f0f0f0"]]
        },
        "TCheckbutton": {
          "foreground": [["disabled", "#8a8a8a"], ["!disabled", "#111111"]],
          "background": [["disabled", "#e2e2e2"], ["active", "#e8e8e8"], ["!disabled", "#f0f0f0"]]
        },
        "TCombobox": {
          "foreground": [["disabled", "#8a8a8a"], ["!disabled", "#111111"]],
          "fieldbackground": [["disabled", "#e3e3e3"], ["active", "#f6f6f6"], ["!disabled", "#ffffff"]],
          "background": [["disabled", "#e0e0e0"], ["active", "#e6e6e6"], ["!disabled", "#f0f0f0"]]
        },
        "TEntry": {
          "foreground": [["disabled", "#8a8a8a"], ["!disabled", "#111111"]],
          "fieldbackground": [["disabled", "#e5e5e5"], ["!disabled", "#ffffff"]]
        },
        "TSpinbox": {
          "foreground": [["disabled", "#8a8a8a"], ["!disabled", "#111111"]],
          "fieldbackground": [["disabled", "#e5e5e5"], ["!disabled", "#ffffff"]],
          "background": [["disabled", "#e0e0e0"], ["active", "#e6e6e6"], ["!disabled", "#f0f0f0"]]
        },
        "TNotebook.Tab": {
          "foreground": [["selected", "#111111"], ["active", "#111111"], ["disabled", "#8a8a8a"], ["!selected", "#111111"]],
          "background": [["selected", "#ffffff"], ["active", "#d8ebff"], ["disabled", "#e5e5e5"], ["!selected", "#e6e6e6"]]
        }
      },
      "options": {
        "*Listbox.background": "#ffffff",
        "*Listbox.foreground": "#111111",
        "*Listbox.selectBackground": "#cde4ff",
        "*Listbox.selectForeground": "#000000"
      }
    },
